{
  "name": "review",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "createAt": {
      "type": "date",
      "required": true
    },
    "comment": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "appuser": {
      "type": "belongsTo",
      "model": "appuser",
      "foreignKey": "userId"
    },
    "content": {
      "type": "belongsTo",
      "model": "content",
      "foreignKey": "contentId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
