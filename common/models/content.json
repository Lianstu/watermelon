{
  "name": "content",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "createAt": {
      "type": "date",
      "required": true
    },
    "mycontent": {
      "type": "string",
      "required": true
    },
    "count": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "appusers": {
      "type": "belongsTo",
      "model": "appuser",
      "foreignKey": "userId"
    },
    "reviews": {
      "type": "hasMany",
      "model": "review",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
